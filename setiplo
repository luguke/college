package setiplo;

/**
 *
 * @author luguq
 */
public class setiplo {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int numero = 2;
        if (numero % 7 ==0){
                System.out.println("Sétiplo");
        }
        else{
            System.out.println("Não Sétiplo");
    }
    }
    
}
