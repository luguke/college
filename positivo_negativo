package negpos;

/**
 *
 * @author luguq
 */
public class NegPos {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int numero = 5;
        if (numero >= 0){
                System.out.println("Positivo");
        }
        else{
            System.out.println("Negativo");
    }
    }
    
}
